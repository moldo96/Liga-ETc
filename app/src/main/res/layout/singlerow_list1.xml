<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:id="@+id/txt_prof"
        android:layout_alignBaseline="@+id/txt_numematerie"
        android:layout_alignBottom="@+id/txt_numematerie"
        android:layout_toEndOf="@+id/txt_numematerie" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="55dp"
        android:id="@+id/txt_numematerie"
        android:layout_alignBottom="@+id/imageView0"
        android:layout_toEndOf="@+id/imageView0" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/txt_numematerie"
        android:layout_alignParentStart="true"
        android:layout_marginStart="10dp"
        android:id="@+id/txt_ora" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logomic"
        android:id="@+id/imageView0"
        android:layout_marginStart="20dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/txt_ora" />
</RelativeLayout>